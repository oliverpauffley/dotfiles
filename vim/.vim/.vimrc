set nocompatible
filetype off

" set the runtime path to include Vundle and start
set rtp +=~/.vim/bundle/Vundle.vim
call vundle#begin()

" let Vundle manage Vundle
Plugin 'VundleVim/Vundle.vim'





" All Plugins must be added before this line!
call vundle#end()
filetype plugin indent on


"brief help 

":PluginList -list all plugins
":PluginInstall - install plugins and update
":PluginSearch foo - searches for foo plugin
":PluginClean -removes unused plugins
